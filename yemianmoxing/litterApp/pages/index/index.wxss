/*
  首页css
*/
/* 查询 */
.search {
    width: 100%;
    height: 92rpx;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 9;
    background: rgba(0,0,0,.2);
}
.search-inner {
    width: 92%;
    height: 64rpx;
    margin-top: 14rpx;
}
.search-r {
    width: 90%;
    height: 64rpx;
    background-color: #f8f8f8;
    border-radius: 40rpx;
}
.search-r input {
    background: #f8f8f8 url(data:image/png;base64,) no-repeat 25rpx 18rpx;
    background-size: 30rpx;
    background-repeat: no-repeat;
    color: #898989;
    width: 100%;
    height: 64rpx;
    line-height: 64rpx;
    border-radius: 40rpx;
    font-size: 28rpx;
    text-indent: 65rpx;
}
/* 导航 */
.seclist {
    padding-top: 100rpx;
    padding-bottom: 110rpx;
}
/*商品推荐 begin*/
.pro-top {
    width: 520rpx;
    height: 30rpx;
    line-height: 30rpx;
    position: relative;
    color: #888888;
    font-size: 30rpx;
    margin: 27rpx auto;
}
.protop-name {
    position: absolute;
    width: 194rpx;
    height: 30rpx;
    left: 163rpx;
    top: 0;
    background: #eeeeee url(data:image/png;base64,) no-repeat 18rpx 4rpx;
    background-size: 26rpx 22rpx;
    z-index: 2;
    text-align: center;
    text-indent: 52rpx;
}
.protop-line {
    position: absolute;
    width: 100%;
    height: 1px;
    background-color: #e0e0e0;
    top: 14.5rpx;
    left: 0;
    z-index: 1;
}
.pro-list navigator {
    width: 49.5%;
    float: left;
    vertical-align: top;
    border: 1px solid #f2f2f2;
    background-color: white;
    margin-bottom: 8rpx;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
}
.pro-list navigator.pro-lilf {
    margin-right: 1%;
}
.pro-img, .pro-img image {
    width: 150rpx;
    height: 150rpx;
}
.pro-img {
    margin: 50rpx auto;
}
.pro-price {
    line-height: 40rpx;
    margin-bottom: 30rpx;
}
.pro-price .fl {
    color: #fd3f3f;
    font-size: 22rpx;
}
.pro-price .fl text {
    font-size: 30rpx;
    font-weight: bold;
}
.pro-price .fr {
    font-size: 24rpx;
    color: #b5b5b5;
    text-decoration: line-through;
}
.plr26 {
    padding-left: 26rpx;
    padding-right: 26rpx;
}
.pro-name {
    color: #222;
    font-size: 28rpx;
    line-height: 40rpx;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}
/* 清楚浮动 */
.fr{float: right;}
.clearfix{zoom: 1; clear:both;}
.clearfix:after {display: block;height: 0;overflow: hidden;content: " ";visibility: hidden;clear: both;}